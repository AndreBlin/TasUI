{
  "name": "iotreboot-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "electron-is-dev": "^1.1.0",
    "ip-address": "^6.1.0",
    "node": "^13.1.0",
    "notistack": "^0.9.6",
    "react": "^16.12.0",
    "react-console-emulator": "^3.0.2",
    "react-dom": "^16.12.0",
    "react-inlinesvg": "^1.1.7",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-scrollbars-custom": "^4.0.21",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "@babel/helper-create-class-features-plugin": "^7.7.4",
    "concurrently": "^5.0.0",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "typescript": "^3.7.2",
    "wait-on": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder -w -c.mac.identity=null"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "author": {
    "name": "IOTReboot",
    "email": "iotreboot@shantur.com",
    "url": "http://iotreboot.shantur.com"
  },
  "build": {
    "appId": "com.shantur.iotreboot",
    "productName": "IOTReboot UI",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "zip"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
